<p id="notice"><%= notice %></p>

<h1>Atm Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atm_brands.each do |atm_brand| %>
      <tr>
        <td><%= atm_brand.name %></td>
        <td><%= atm_brand.description %></td>
        <td><%= link_to 'Show', atm_brand %></td>
        <td><%= link_to 'Edit', edit_atm_brand_path(atm_brand) %></td>
        <td><%= link_to 'Destroy', atm_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atm Brand', new_atm_brand_path %>
