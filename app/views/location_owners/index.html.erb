<p id="notice"><%= notice %></p>

<h1>Location Owners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Business name</th>
      <th>Document number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_owners.each do |location_owner| %>
      <tr>
        <td><%= location_owner.name %></td>
        <td><%= location_owner.business_name %></td>
        <td><%= location_owner.document_number %></td>
        <td><%= link_to 'Show', location_owner %></td>
        <td><%= link_to 'Edit', edit_location_owner_path(location_owner) %></td>
        <td><%= link_to 'Destroy', location_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location Owner', new_location_owner_path %>
